    "start": "NODE_ENV=production node ./dist/server.js",
    "test": "jest --detectOpenHandles --verbose",
    "type-check": "tsc --build tsconfig.json",
    "validate": "yarn lint && yarn type-check && yarn build",
    "watch": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --copy-files --watch"
